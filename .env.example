# Environment Variables for Political News Digest System
# Copy this file to .env and fill in your actual values

# Required: Google Gemini API Key
# Get from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Discord Webhook URLs for each digest type
# Create webhooks in your Discord server settings > Integrations > Webhooks

# Belgium politics digest webhook
BE_POLITICS_DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/your_belgium_politics_webhook_url

# World politics/news digest webhook
WORLD_NEWS_DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/your_world_news_webhook_url
